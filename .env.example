# MDS Configuration
MDS_PROVIDER_ID=kiwibot-delivery-robots
MDS_VERSION=2.0.0

# Authentication
JWT_SECRET_KEY=your-jwt-secret-key-here
AUTH0_DOMAIN=your-auth0-domain.auth0.com
AUTH0_AUDIENCE=your-auth0-audience

# BigQuery Configuration
BIGQUERY_PROJECT_ID=kiwibot-atlas
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Redis Configuration
REDIS_URL=redis://localhost:6379

# API Configuration
CORS_ORIGINS=["*"]
API_PREFIX=/v1/provider

# Cache TTL (seconds)
CACHE_TTL_VEHICLES=60
CACHE_TTL_TRIPS=3600
CACHE_TTL_EVENTS=300

# Data Filtering
MIN_LOCATION_ACCURACY=0.7
VEHICLE_RETENTION_DAYS=30
EVENT_RETENTION_DAYS=14

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=60

# Logging
LOG_LEVEL=INFO
DEBUG=false